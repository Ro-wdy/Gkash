<?xml version="1.0" encoding="UTF-8"?>
<svg width="270" height="570" viewBox="0 0 270 570" xmlns="http://www.w3.org/2000/svg">
    <!-- Phone screen background -->
    <rect width="270" height="570" fill="#1a1a1a"/>
    
    <!-- Status bar -->
    <rect x="0" y="0" width="270" height="30" fill="#0d0d0d"/>
    <text x="10" y="20" font-family="Arial" font-size="12" fill="white">21:21</text>
    <text x="240" y="20" font-family="Arial" font-size="12" fill="white">91%</text>
    
    <!-- Chat Header -->
    <rect x="0" y="30" width="270" height="60" fill="#2a2a2a"/>
    <circle cx="40" cy="60" r="18" fill="#E91E63"/>
    <text x="65" y="55" font-family="Arial" font-size="16" font-weight="600" fill="white">
        Financial AI Assistant
    </text>
    <circle cx="65" cy="70" r="4" fill="#4CAF50"/>
    <text x="75" y="73" font-family="Arial" font-size="12" fill="#4CAF50">Online</text>
    
    <!-- Chat Messages -->
    <!-- User message -->
    <rect x="100" y="110" width="150" height="60" rx="15" fill="#E91E63"/>
    <text x="115" y="135" font-family="Arial" font-size="14" fill="white">
        How should I start
    </text>
    <text x="115" y="155" font-family="Arial" font-size="14" fill="white">
        budgeting?
    </text>
    
    <!-- AI Response -->
    <rect x="20" y="190" width="200" height="140" rx="15" fill="#2a2a2a"/>
    <text x="35" y="215" font-family="Arial" font-size="13" fill="white">
        Start by tracking your income
    </text>
    <text x="35" y="235" font-family="Arial" font-size="13" fill="white">
        and expenses for a month to
    </text>
    <text x="35" y="255" font-family="Arial" font-size="13" fill="white">
        see where your money goes.
    </text>
    <text x="35" y="275" font-family="Arial" font-size="13" fill="white">
        Then, create a budget allocating
    </text>
    <text x="35" y="295" font-family="Arial" font-size="13" fill="white">
        funds for essentials, savings,
    </text>
    <text x="35" y="315" font-family="Arial" font-size="13" fill="white">
        and discretionary spending.
    </text>
    
    <!-- User thank you -->
    <rect x="150" y="350" width="100" height="40" rx="15" fill="#E91E63"/>
    <text x="170" y="375" font-family="Arial" font-size="14" fill="white">thank you</text>
    
    <!-- AI Response 2 -->
    <rect x="20" y="410" width="200" height="80" rx="15" fill="#2a2a2a"/>
    <text x="35" y="435" font-family="Arial" font-size="13" fill="white">
        You're welcome! Remember,
    </text>
    <text x="35" y="455" font-family="Arial" font-size="13" fill="white">
        money market funds offer
    </text>
    <text x="35" y="475" font-family="Arial" font-size="13" fill="white">
        good starting points.
    </text>
    
    <!-- Input area -->
    <rect x="20" y="510" width="230" height="40" rx="20" fill="#2a2a2a"/>
    <text x="35" y="535" font-family="Arial" font-size="14" fill="#666">
        Ask about your finances...
    </text>
    <circle cx="220" cy="530" r="15" fill="#E91E63"/>
    
    <!-- Bottom navigation -->
    <rect x="0" y="520" width="270" height="50" fill="#2a2a2a" opacity="0.8"/>
    <text x="30" y="550" font-family="Arial" font-size="11" fill="white">Home</text>
    <text x="80" y="550" font-family="Arial" font-size="11" fill="white">Learn</text>
    <circle cx="135" cy="545" r="20" fill="#4CAF50"/>
    <text x="125" y="550" font-family="Arial" font-size="10" fill="white">GAME</text>
    <text x="180" y="550" font-family="Arial" font-size="11" fill="#E91E63">Chat</text>
    <text x="230" y="550" font-family="Arial" font-size="11" fill="white">Profile</text>
</svg>
